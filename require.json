{
    "baseUrl": ".",
    "paths": {
        "app": "src/utils/app",
        "helpers": "src/utils/helpers",
        "jquery": "lib/jquery/jquery",
        "lodash": "lib/lodash/lodash.compat",
        "backbone": "lib/backbone/backbone",
        "document": "src/utils/helpers/document",
        "window": "src/utils/helpers/window"
    },
    "shim": {
        "jquery": {
            "exports": "$"
        },
        "lodash": {
            "exports": "_"
        },
        "backbone": {
            "deps": [ "jquery", "lodash" ],
            "exports": "Backbone"
        }
    }
}
