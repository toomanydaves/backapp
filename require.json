{
    "baseUrl": ".",
    "paths": {
        "backbone": "lib/backbone/backbone",
        "jquery": "lib/jquery/jquery",
        "lodash": "lib/lodash/lodash.compat"
    },
    "shim": {
        "jquery": {
            "exports": "$"
        },
        "lodash": {
            "exports": "_"
        },
        "backbone": {
            "deps": [ "jquery", "lodash" ],
            "exports": "Backbone"
        }
    }
}
